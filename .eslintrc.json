{
  "extends": ["plugin:prettier/recommended"],
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "comma-dangle": ["error", "never"],
    "arrow-parens": ["error", "as-needed"],
    "prettier/prettier": "error"
  }
}
